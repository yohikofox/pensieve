{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=\"src/.*\\.test\\.ts$\"",
    "test:acceptance": "jest --config jest.config.acceptance.js",
    "test:acceptance:watch": "jest --config jest.config.acceptance.js --watch",
    "test:acceptance:story-2-1": "jest --config jest.config.acceptance.js --testPathPatterns=\"tests/acceptance/story-2-1-simple\\.test\\.ts$\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@react-navigation/stack": "^7.6.16",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "expo": "~54.0.31",
    "expo-apple-authentication": "^8.0.8",
    "expo-asset": "~12.0.12",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "~7.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-linking": "~8.0.11",
    "expo-network": "^8.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "babel-preset-expo": "^54.0.9",
    "better-sqlite3": "^12.6.2",
    "detox": "^20.28.3",
    "jest": "^30.2.0",
    "jest-cucumber": "^4.5.0",
    "jest-expo": "^54.0.16",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.9.2",
    "uuid": "^11.0.3"
  },
  "private": true
}
