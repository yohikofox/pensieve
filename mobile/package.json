{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "APP_VARIANT=dev expo run:android",
    "ios": "APP_VARIANT=dev expo run:ios",
    "android:release": "APP_VARIANT=release expo run:android",
    "ios:release": "APP_VARIANT=release expo run:ios",
    "web": "expo start --web",
    "prebuild": "APP_VARIANT=dev expo prebuild",
    "prebuild:clean": "APP_VARIANT=dev expo prebuild --clean",
    "prebuild:release": "APP_VARIANT=release expo prebuild",
    "prebuild:release:clean": "APP_VARIANT=release expo prebuild --clean",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=\"src/.*\\.test\\.ts$\"",
    "test:acceptance": "jest --config jest.config.acceptance.js",
    "test:acceptance:watch": "jest --config jest.config.acceptance.js --watch",
    "test:acceptance:story-2-1": "jest --config jest.config.acceptance.js --testPathPatterns=\"tests/acceptance/story-2-1-simple\\.test\\.ts$\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "wipe:ios": "xcrun simctl shutdown booted && xcrun simctl erase all && npm run ios",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@kesha-antonov/react-native-background-downloader": "^4.4.5",
    "@op-engineering/op-sqlite": "^15.2.3",
    "@preeternal/react-native-file-hash": "^1.1.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^5.1.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@react-navigation/stack": "^7.6.16",
    "@supabase/supabase-js": "^2.90.1",
    "@types/jest": "29.5.14",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^17.2.3",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "~7.0.10",
    "expo-background-fetch": "^14.0.9",
